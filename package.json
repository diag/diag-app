{
  "name": "diag-app",
  "description": "Diag app library",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "tsc --listFiles -p ./tsconfig.json",
    "test": "NODE_ENV=test node_modules/.bin/jest -i",
    "magic": "npm version patch && npm run build && npm publish && git push"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "text-encoding": "^0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diag/diag-app.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^9.4.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "es6-promise": "^4.0.5",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-parallel": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "gdi-http": "git+ssh://git@github.com/taktakhq/gdi-http.git",
    "jest-cli": "^20.0.4",
    "node-fetch": "^2.0.0-alpha.9",
    "redux-mock-store": "^1.3.0",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "src/**/*.{ts}",
      "src/**/*.{tsx}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  }
}
